{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "username": {
      "type": "string"
    },
    "title": {
      "type": ["null","string"]
    },
    "firstName": {
      "type": ["null","string"]
    },
    "lastName": {
      "type": ["null","string"]
    },
    "email": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "language": {
      "type": "string"
    },
    "sdkEntity": {
      "type": "string"
    },
    "sdkPartner": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "updatedAt": {
      "type": "string"
    },
    "enabled": {
      "type": "boolean"
    },
    "partnerId": {
      "type": "string"
    },
    "applicationId": {
      "type": "null"
    },
    "timesLoggedIn": {
      "type": "integer"
    },
    "roles": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "role": {
              "type": "string"
            }
          },
          "required": [
            "role"
          ]
        }
      ]
    },
    "socialProviders": {
      "type": "null"
    },
    "guest": {
      "type": "boolean"
    },
    "accountNonExpired": {
      "type": "boolean"
    },
    "accountNonLocked": {
      "type": "boolean"
    },
    "credentialsNonExpired": {
      "type": "boolean"
    },
    "timezone": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "username",
    "title",
    "firstName",
    "lastName",
    "email",
    "phone",
    "language",
    "sdkEntity",
    "sdkPartner",
    "createdAt",
    "updatedAt",
    "enabled",
    "partnerId",
    "applicationId",
    "timesLoggedIn",
    "roles",
    "socialProviders",
    "guest",
    "accountNonExpired",
    "accountNonLocked",
    "credentialsNonExpired",
    "timezone"
  ]
}